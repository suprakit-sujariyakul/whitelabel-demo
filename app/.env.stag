# Staging Environment Configuration
NODE_ENV=staging
PUBLIC_API_URL=https://api-stag.whitelabel-demo.com
PUBLIC_WS_URL=wss://api-stag.whitelabel-demo.com
PUBLIC_CLIENT_ID=client-a
PUBLIC_ENVIRONMENT=stag

# Database
DATABASE_URL=postgresql://staging-db.whitelabel-demo.com:5432/whitelabel_stag

# Auth
JWT_SECRET=staging-jwt-secret-please-change
SESSION_SECRET=staging-session-secret-please-change

# External Services
GOOGLE_ANALYTICS_ID=GA-STAG-TRACKING-ID
STRIPE_PUBLISHABLE_KEY=pk_test_stag_key
STRIPE_SECRET_KEY=sk_test_stag_key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_PAYMENTS=true
ENABLE_LOBBY=true
ENABLE_SCREENING=true

# Logging
LOG_LEVEL=info
