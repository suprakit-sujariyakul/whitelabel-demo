# Production Environment Configuration
NODE_ENV=production
PUBLIC_API_URL=https://api.whitelabel-demo.com
PUBLIC_WS_URL=wss://api.whitelabel-demo.com
PUBLIC_CLIENT_ID=client-a
PUBLIC_ENVIRONMENT=prod

# Database
DATABASE_URL=postgresql://prod-db.whitelabel-demo.com:5432/whitelabel_prod

# Auth
JWT_SECRET=production-jwt-secret-must-be-changed
SESSION_SECRET=production-session-secret-must-be-changed

# External Services
GOOGLE_ANALYTICS_ID=GA-PROD-TRACKING-ID
STRIPE_PUBLISHABLE_KEY=pk_live_prod_key
STRIPE_SECRET_KEY=sk_live_prod_key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_PAYMENTS=true
ENABLE_LOBBY=true
ENABLE_SCREENING=true

# Logging
LOG_LEVEL=warn
